		{
			"name": "Camera 1 (Delayed Start - 20 seconds)",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"  // wait 20 seconds or so before beginning execution\r",
							"  var date = new Date();\r",
							"  var curDate = null;\r",
							"  do {\r",
							"    curDate = new Date();\r",
							"  }\r",
							"  while (curDate - date < 20000);"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n    \"source\": {\r\n        \"uri\": \"{{camera1}}\",\r\n        \"type\": \"uri\"\r\n    },\r\n    \"destination\": {\r\n        \"metadata\": {\r\n            \"type\": \"file\",\r\n            \"path\": \"/tmp/results/results_1.json\"\r\n        },\r\n        \"frame\": {\r\n            \"type\": \"rtsp\",\r\n   \"sync-with-source\": false,\r\n           \"path\": \"camera_1\"\r\n        }\r\n    },\r\n    \"parameters\": {\r\n        \"classification\": {\r\n            \"device\": \"CPU\"\r\n        },\r\n        \"detection\": {\r\n            \"device\": \"CPU\"\r\n        }\r\n    }\r\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{host}}/pipelines/{{pipelineName}}",
					"host": [
